<div class="contextual">
  <%= link_to l(:button_show), business_contact_path(@organization), class: "icon icon-show" %>
  <%= link_to l(:button_edit), edit_business_contact_path(@organization), class: "icon icon-edit", :accesskey => accesskey(:edit) %>
  <%= link_to l(:button_delete), business_contact_path(@organization), class: "icon icon-del", method: :delete, data: { confirm: l(:business_contact_destroy_confirmation_message) } %>
</div>